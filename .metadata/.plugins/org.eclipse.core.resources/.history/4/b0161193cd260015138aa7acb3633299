package main;

public class Solution {
	public boolean exist(char[][] board, String word) {
        
		
		return false;
    }
	
	
	
	public boolean backtrack(String word, String part, char[][] board, int[] pos ) {
		if(part.equals(word))
			return true;
		
		//up
		if(pos[0] != 0 ||word.charAt(part.length()) == board[pos[0]-1][pos[1]]) {
			part = part + board[pos[0]-1][pos[1]];
			pos[0]--;
		}
		
		//down
		if(pos[0] != board.length ||word.charAt(part.length()) == board[pos[0]-1][pos[1]]) {
			part = part + board[pos[0]-1][pos[1]];
			pos[0]--;
		}
		
		
		
		return false;
	}

	
	public static void main(String[] args) {
		int board[][] = {{1,2,3},{1,2,3}};
		System.out.println(board.length);
	}
}
