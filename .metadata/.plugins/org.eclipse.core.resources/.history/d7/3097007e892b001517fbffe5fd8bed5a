package main;

public class Solution {
    public boolean isValidSudoku(char[][] board) {
        
    	boolean used[] = new boolean[9];
    	//check every row
    	for(int i = 0 ; i < board.length; i++ ) {
    		for(int j = 0 ; j < board.length; j++) {
    			if(board[i][j] == '.')
    				continue;
    			if(used[board[i][j]-'0'])
    				return false;
    			used[board[i][j]-'0'] = true;
    		}
    		
    		for(int j = 0 ; j < 9; j++)
    			used[j] = false;
    	}
    	
    	//check every column
    	for(int i = 0 ; i < board.length; i++ ) {
    		for(int j = 0 ; j < board.length; j++) {
    			if(board[j][i] == '.')
    				continue;
    			if(used[board[j][i]-'0'])
    				return false;
    			used[board[j][i]-'0'] = true;
    		}
    		
    		for(int j = 0 ; j < 9; j++)
    			used[j] = false;
    	}
    	
    	
    }
}