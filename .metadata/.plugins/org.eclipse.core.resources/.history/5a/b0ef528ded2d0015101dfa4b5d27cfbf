package main;

public class Solution {
	public static int jump(int[] nums) {
		if(nums == null || nums.length==0)
			return 0;
        int curFarthest = nums.length-1;
        int res = 0;
        while(curFarthest != 0) {
        	for(int j = 0; j < nums.length; j++)
        		if(nums[j] + j >= curFarthest) {
        			curFarthest = j;
        			res++;
        			break;
        		}       	
        }
        return res;
    }
}
