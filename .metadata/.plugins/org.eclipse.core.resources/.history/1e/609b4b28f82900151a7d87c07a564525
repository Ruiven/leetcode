package main;

class ListNode {
	int val;
	ListNode next;
	ListNode(int x) { val = x; }
}

public class Solution {
	public ListNode reverseKGroup(ListNode head, int k) {
		ListNode newHead = null, p = head, q = null;
		for(int i = 0 ; i < k-1; i++ ) 
			p = p.next;
		newHead = p;
		
		ListNode nextHead = p.next;
		
		
		
	}
}