package main;

public class Solution {
	public String countAndSay(int n) {
        String start = "1";
        String temp = "";
        for(int i= 1; i < n; i++) {
        	int last = 0;
        	for(int j = 0; j < start.length(); j++) {
        		if(j > 0 && start.charAt(j) != start.charAt(j-1)) {
        			temp = temp + (char)('0'+ j-last);
        			temp = temp + start.charAt(j-1);
        			last = j;
        		}
        		if(j == start.length()-1) {
        			temp = temp + (char)('0'+ j-last);
        		}
        		
        		
        		
        	}
        }
        
        return start;
    }
}
