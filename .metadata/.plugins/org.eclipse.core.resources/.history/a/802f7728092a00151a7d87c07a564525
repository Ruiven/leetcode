package main;

public class Solution {
	public static int removeDuplicates(int[] nums) {
		int res = 0;
		
        for(int i = 0 ; i < nums.length; i++) {
        	if(i > 0 && nums[i] == nums[i-1])
        		continue;
        	res++;        	
        }
        return res;
    }
	
	public static void main(String[] args) {
		int nums[] = {1,1,2};
		int l = removeDuplicates(nums);
		for(int i = 0 ; i < l; i++)
			System.out.print(nums[i]);
	}
}
