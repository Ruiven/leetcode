package main;

public class Solution {
	public String intToRoman(int num) {
        String roman = "";
		int numOfThou = num/1000;
        for(int i = 0 ; i < numOfM; i++)
        	roman = roman + "M";
        
        int numOfHund = (num-numOfThou*1000)/100;
        if(numOfHund == 9) {
        	roman = roman + "CM";
        }
        else if(numOfHund >= 5) {
        	roman = roman + "D";
        	for(int i = 0; i < numOfHund-5; i++)
        		roman = roman + "C";
        }
        else {
        	for(int i = 0; i < numOfHund; i++)
        		roman = roman + "C";
        }
        
        
    }
}
