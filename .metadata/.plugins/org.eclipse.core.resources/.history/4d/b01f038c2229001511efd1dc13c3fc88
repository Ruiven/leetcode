package main;

public class Solution {
	public static int reverse(int x) {
        int copy = x;
        //x = x >0 ? x : -x;
		int res = 0 ;
        int div = 10;
        
        int max = 2147483647;
        if(x > 0) {
	        while(x != 0) {
	        	res *= 10;
	        	res += x % 10;
	        	if(res > max/10 || res == max/10 && (x/10)%100 > 7)
	        		return 0;
	        	x /= 10;
	        }
        }
        
        else {
        	while(x != 0) {
	        	res *= 10;
	        	res += x % 10;
	        	if(res < max/10 || res == max/10 && (x/10)%100 < -8)
	        		return 0;
	        	x /= 10;
	        }
        	
        }
        
        if(copy < 0)
        	x = -x;  
        
        return res;
		
    }
	public static void main(String[] args) {
		System.out.print(reverse(-2147483648));
	}
}
