package main;

public class Solution {
	public static int reverse(int x) {
        int copy =x;
		int res = 0 ;
        int div = 10;
        while(x != 0) {
        	res *= 10;
        	res += x % 10;
        	x /= 10;
        }
        if(copy > 0 && res < 0)
        	return 0;
        if(copy < 0 && res > 0)
        	return 0;
        
        return res;
		
    }
	public static void main(String[] args) {
		System.out.print(reverse(1000000003));
	}
}
