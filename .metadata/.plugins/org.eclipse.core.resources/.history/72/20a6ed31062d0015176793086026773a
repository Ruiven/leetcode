package main;

public class Solution {
	public static int jump(int[] nums) {
        int curFarthest = nums.length-1;
        int res = 0;
        while(curFarthest != 0) {
        	for(int j = 0; j < nums.length; j++)
        		if(nums[j] + j >= curFarthest) {
        			curFarthest = j;
        			res++;
        			break;
        		}       	
        }
        return res;
    }
	
	public static void main(String[] args) {
		int nums[] = {2,3,1,1,4};
		System.out.print(jump(nums));
	}
}
