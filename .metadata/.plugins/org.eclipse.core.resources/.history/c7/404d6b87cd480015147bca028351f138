package main;

import java.util.ArrayList;
import java.util.LinkedList;
import java.util.List;


class TreeNode {
	int val;
	TreeNode left;
	TreeNode right;
	TreeNode(int x) { val = x; }
}

public class Solution {
	public int sumNumbers(TreeNode root) {
		if(root == null)
			return 0;
		int sum = sumNumbers(root.left) + sumNumbers(root.right);
     }
	
	int recursive(TreeNode root, int level[]) {
		if(root == null) {
			level[0] = 0;
			return 0;
		}
		
		int total = 0;
		int left = recursive(root.left, level);
		total += root.val * Math.pow(10, level[0]) + left;
		
	}
	
	
}
