package main;

import java.util.Arrays;
import java.util.Collections;

public class Solution {
	public int firstMissingPositive(int[] nums) {
		Arrays.sort(nums);
		for(int i = 0 ; i < nums.length-1; i++) {
			if(nums[i] >= 0 && nums[i+1] != nums[i]+1)
				return nums[i]+1;
			
		}
		
		if()
		
    }
}
