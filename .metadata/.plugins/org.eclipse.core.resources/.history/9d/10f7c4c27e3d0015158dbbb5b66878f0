package main;

import java.util.Stack;

class TreeNode {
	int val;
	TreeNode left;
	TreeNode right;
	TreeNode(int x) { val = x; }
}

public class Solution {
	public boolean isBalanced(TreeNode root) {
        if(root == null)
        	return true;
        
        		
    }
	
	int depth(TreeNode root) {
		if(root == null)
			return 0;
		
		return Math.max(root.left, root.right)+1;
	}
	
	
}
