package main;

public class Solution {
	public int jump(int[] nums) {
        int curFarthest = nums.length-1;
        int res = 0;
        int i = nums.length-1;
        while(curFarthest != 0) {
        	for(int j = 0; j < nums.length; j++)
        		if(nums[j] + j >= curFarthest) {
        			curFarthest = j;
        			break;
        		}       	
        }
    }
}
