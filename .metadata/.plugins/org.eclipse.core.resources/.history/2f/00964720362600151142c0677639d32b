package main;

import java.util.ArrayList;
import java.util.List;

public class Solution {
	
	public List<Integer> part;
	
	public List<List<String>> solveNQueens(int n) {
		part = new ArrayList<Integer>();
		return null;
    }
	
	public boolean contradict(int row, int column, int n) {
		return true;
	}
	
	
	
	public void backtrack(int row, int n, List<List<Integer>> solutions) {
		if(row == n) {
			solutions.add(new ArrayList<Integer>(part));
			return;
		}
		
		for(int i = 0 ; i < n; i++) {
			if(contradict(row, i, n))
				continue;
			part.add(i);
			backtrack(row+1, n, solutions);
			part.remove(part.size()-1);
		}
		
	}
}
