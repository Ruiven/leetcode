package main;

import java.util.Set;

public class Solution {
	int minStep;
	public int ladderLength(String beginWord, String endWord, Set<String> wordDict) {
        minStep = wordDict.size();
    }
	
	void backtrack(String tempWord, String endWord, Set<String> wordDict, int step) {
		if(tempWord.equals(endWord)) {
			minStep = Math.min(minStep, step);
			return;
		}
		
		for(int i = 0; i < wordDict.size(); i++) {
			
		}
		
	}
	
	boolean changeable(String word1, String word2) {	//Assume two words share same length
		int diff = 0;
		for(int i = 0 ; i < word1.length(); i++) {
			if(word1.charAt(i) != word2.charAt(i))
				diff++;
			if(diff > 1)
				return false;
		}
		return true;
	}
}
