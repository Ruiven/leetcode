package main;

public class Solution {
	public static int divide(int dividend, int divisor) {
        long divid = dividend, divis = divisor;
        if(divisor == 1)
        	return dividend;
		
		if(dividend < 0)
        	divid = - dividend;
		if(divisor < 0)
			divis = - divisor;
		
		
		int res = 0;
		
		while(divid >= divis) {
			long a = divis;
			int i;
			for(i = 0 ; a <= divid; i++)
				a <<= 1;
			
			divid -= divis << (i-2);
			res += 1 << (i-2);
		}
		
		if(dividend > 0 && divisor < 0 || dividend < 0 && divisor > 0)
			res = - res;
		return res;
		
    }
	
	public static void main(String[] args) {
		System.out.print(divide(9, 3));
	}
}
