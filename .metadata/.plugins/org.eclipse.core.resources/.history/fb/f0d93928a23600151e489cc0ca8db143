package main;

import java.util.Arrays;

public class Solution {
	public static void merge(int[] nums1, int m, int[] nums2, int n) {
        int i = 0, j = 0;
        int inc = 0;
        while(i < m && j < n) {
        	if(nums1[i] < nums2[j]) {
        		i++;
        		continue;
        	}
        	
        	for(int k = m+inc; k >= i+1; k--) {
        		nums1[k] = nums1[k-1];
        	}
        	
        	inc++;
        	j++;
        	i++;
        }
        
        while(j < n) {
        	nums1[i++] = nums2[j++];
        }
    }
	
	public static void main(String[] args) {
		
	}
}
