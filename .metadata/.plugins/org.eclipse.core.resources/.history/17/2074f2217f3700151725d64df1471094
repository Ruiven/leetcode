package main;

class ListNode {
	int val;
	ListNode next;
	ListNode(int x) { val = x; }
}


public class Solution {
	public ListNode reverseBetween(ListNode head, int m, int n) {
        ListNode newHead = null, newTail = null;
        int i = 1;
        ListNode p = head;
        ListNode leftTail = null, leftHead = null, rightTail = null, rightHead = null;
        ListNode prev = head;
        while(p != null) {
        	if(i == m-1)
        		leftTail = p;
        	if(i == m)
        		rightHead = p;
        	if(i == n)
        		leftTail = p;
        	if(i == n+1)
        		rightTail = p;
        	
        	if(i > m && i <= n) {
        		
        	}
        }
        
        
    }
}
