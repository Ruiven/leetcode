package main;

public class Solution {
	public static int divide(int dividend, int divisor) {
        int divid = dividend, divis = divisor;
		
		if(dividend < 0)
        	divid = - dividend;
		if(divisor < 0)
			divis = - divisor;
		
		
		int res = 0;
		
		while(divid >= divis) {
			divid -= divis;
			res++;
		}
		
		return (int)(dividend < 0 ^ divisor < 0) * res;
		
    }
	
	public static void main(String[] args) {
		System.out.print(divide(-8, 4));
	}
}
