package main;

public class Solution {
    public boolean isPalindrome(int x) {
        int bits = 0;
        int copy = x;
        while(copy != 0) {
        	bits++;
        	copy /= 10;
        }
        
        boolean pal = true;
        int i = 0;
        while(i < bits/2 ) {
        	if(getBit(x, i) != getBit(x, bit-i));
        	
        	
        }
    }
    
    int getBit(int x, int bit) {
    	
    	return (int) (x/Math.pow(10, bit) % 10);
    }
}
