package main;

public class Solution {
	public static double myPow(double x, int n) {
		
	   double res;
	   if(n % 2 == 0)
		   res = x;
	   else
		   res = 1;
	   
       while(n > 1) {
    	   if(n % 2 == 0) {
    		   n /= 2;
    		   res *= res;
    	   }
    	   else{
    		   n -= 1;
    		   res*= x;
    	   }   	   
       }
       return res;
    }
	
	public static void main(String[] args) {
		
	}
}
