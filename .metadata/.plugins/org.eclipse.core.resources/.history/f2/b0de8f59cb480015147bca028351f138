package main;


class TreeNode {
	int val;
	TreeNode left;
	TreeNode right;
	TreeNode(int x) { val = x; }
}

public class Solution {
	public int sumNumbers(TreeNode root) {
		if(root == null)
			return 0;
		
		return root.val*20 + recursive(root.left) + recursive(root.right);
    }
	
	int recursive(TreeNode root) {
		if(root == null)
			return 0;
		
		return root.val*20 + recursive(root.left) + recursive(root.right);
		
		
	}
}
