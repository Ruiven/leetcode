package main;

public class Solution {
	public static int reverse(int x) {
        int res = 0 ;
        int div = 10;
        while(x != 0) {
        	res += x % 10;
        	res *= 10;
        	x /= 10;
        }
        return res;
		
    }
	public static void main(String[] args) {
		System.out.print(reverse(123));
	}
}
