package main;

class ListNode {
	int val;
	ListNode next;
	ListNode(int x) { val = x; }
}

public class Solution {
	public ListNode removeNthFromEnd(ListNode head, int n) {
		ListNode p = head, q = head;
		for(int i = 0 ; i < n; i++)
			q = q.next;
		
		while(q.next != null) {
			p = p.next;
			q = q.next;
		}
		
		p.next = q;
		return head;
        
    }
}
