package main;

public class Solution {
	public static int numDecodings(String s) {
		if(s == null || s.length() == 0 || s.length() == 1)
			return 1;
		int left = numDecodings(s.substring(0, s.length()/2));
		int right = numDecodings(s.substring(s.length()/2));
		int a = s.charAt(s.length()/2-1) - '0';
		int b = s.charAt(s.length()/2) - '0';
		int sum = a*10 + b;
		int middle = 0;
		if(sum <= 26)
			middle = 1 + numDecodings(s.substring(0, s.length()/2-1))*numDecodings(s.substring(s.length()/2+1));
		return left*right+middle;
    }
	
	public static void main(String[] args) {
		System.out.print(numDecodings("12"));
	}
}
