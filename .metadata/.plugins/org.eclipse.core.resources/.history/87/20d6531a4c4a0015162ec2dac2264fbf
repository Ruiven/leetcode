package main;

public class Solution {
	
	public boolean isPalindrome(String s) {
        if(s == null)
        	return false;
        if(s.length() == 0)
        	return true;
        
        int p = 0, q = s.length()-1;
        while(p < q) {
        	if(s.charAt(p) != s.charAt(q))
        		return false;
        	p++;
        	q--;
        }
        return true;
    }
	
	
	public List<List<String>> partition(String s) {
        
    }
}
